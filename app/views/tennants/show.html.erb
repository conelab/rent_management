<div class="page-header">
  <%= link_to tennants_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_tennant_path(@tennant), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show tennant</h1>
</div>

<dl class="dl-horizontal">
  <dt>Property:</dt>
  <dd><%= @tennant.property_id %></dd>

  <dt>Name:</dt>
  <dd><%= @tennant.name %></dd>

  <dt>Username:</dt>
  <dd><%= @tennant.username %></dd>

  <dt>Encrypted password:</dt>
  <dd><%= @tennant.encrypted_password %></dd>

  <dt>Email:</dt>
  <dd><%= @tennant.email %></dd>

  <dt>Phone number:</dt>
  <dd><%= @tennant.phone_number %></dd>

  <dt>Address:</dt>
  <dd><%= @tennant.address %></dd>

  <dt>City:</dt>
  <dd><%= @tennant.city %></dd>

  <dt>Country:</dt>
  <dd><%= @tennant.country %></dd>

  <dt>Is active:</dt>
  <dd><%= @tennant.is_active %></dd>

</dl>
